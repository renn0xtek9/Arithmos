[run]
parallel = 1
concurrency =
    multiprocessing
    thread
source =
    Arithmos
omit =
    Arithmos/canvas/*
    Arithmos/tests/*
    */setup.py
    */*/setup.py

[report]
exclude_lines =
    pragma: no cover
    raise NotImplementedError
    if __name__ == .__main__.:
    except MemoryError
    assert False
    raise AssertionError
